<%-include('../partials/header')%>
<div class="container">
    <p><%= u.userName %></p>
    <hr>

    <%u.photos.forEach(function(p){%>
    <div class="posts">
        <p><%=p.userName%></p>
        <img style="width: 100%; height: 55%;" src="<%=p.photo%>">
        <div>
            <a href="#"></a><img style="width: 20px; height: 20px; margin: 3px 5px;" src="/images/likeBtn.jpeg"></a>
            <a href="#"></a><img style="width: 20px; height: 25px; margin: 0 5px;" src="/images/commentBtn.jpeg"></a>
        </div>
        <p><strong><%=p.userName%>: </strong><%=p.caption%></p>
    </div>
    <%})%>

</div>



<%-include('../partials/footer')%>